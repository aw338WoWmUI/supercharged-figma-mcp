{
  "name": "supercharged-figma-mcp",
  "version": "1.0.6",
  "description": "Supercharged Figma MCP Server with full automation support",
  "main": "dist/server.js",
  "type": "module",
  "files": [
    "bin/supercharged-figma-mcp.js",
    "dist/server.js",
    "dist/enhanced-batch-operations.js",
    "dist/progress-manager.js",
    "dist/rest-bridge.js",
    "dist/types.js",
    "dist/runtime/embedded-relay.js",
    "dist/runtime/instance-manager.js",
    "README.md",
    "SECURITY.md",
    "USER_GUIDE.md",
    "DEVELOPER_GUIDE.md",
    "relay-server.js",
    "setup-mcp.sh"
  ],
  "bin": {
    "supercharged-figma-mcp": "./bin/supercharged-figma-mcp.js"
  },
  "scripts": {
    "build": "npm run build:mcp && npm run build:plugin",
    "build:mcp": "tsc",
    "build:plugin": "tsc -p figma-plugin/tsconfig.json --noCheck",
    "plugin:build": "npm run build:plugin",
    "typecheck:plugin": "tsc -p figma-plugin/tsconfig.json --noEmit --noCheck false",
    "plugin:typecheck": "npm run typecheck:plugin",
    "dev": "tsc --watch",
    "start": "node dist/server.js --local",
    "start:http": "node dist/server.js --local --transport http --host 127.0.0.1 --port 3333 --mcp-path /mcp",
    "start:remote": "node dist/server.js --remote ws://127.0.0.1:8888",
    "lint": "tsc --noEmit",
    "test": "node --test dist/tests/*.js",
    "test:unit": "node --test dist/tests/unit/*.js",
    "test:integration": "node --test dist/tests/integration/*.js",
    "validate": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "chalk": "^5.3.0",
    "readline": "^1.3.0",
    "uuid": "^11.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.123.0",
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.13",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "figma",
    "mcp",
    "ai",
    "design",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aw338WoWmUI/supercharged-figma-mcp"
  },
  "homepage": "https://github.com/aw338WoWmUI/supercharged-figma-mcp",
  "bugs": {
    "url": "https://github.com/aw338WoWmUI/supercharged-figma-mcp/issues"
  }
}
